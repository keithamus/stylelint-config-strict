{
  "name": "stylelint-config-strict",
  "description": "Stylelint sharable config for strict linting",
  "keywords": [
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "csslint",
    "enforce",
    "hint",
    "lint",
    "policy",
    "quality",
    "simple",
    "strict",
    "strict style",
    "style",
    "style checker",
    "style linter",
    "stylelint",
    "stylelintconfig",
    "verify"
  ],
  "license": "MIT",
  "author": "Keith Cirkel <npm@keithcirkel.co.uk> (http://keithcirkel.co.uk/)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/keithamus/stylelint-config-strict.git"
  },
  "scripts": {
    "test": "eslint .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ]
    }
  },
  "stylelint": {
    "extends": "./index.js"
  },
  "dependencies": {
    "stylelint": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "ghooks": "^1.0.1",
    "semantic-release": "^4.3.5",
    "travis-after-all": "^1.4.4",
    "validate-commit-msg": "^1.0.0"
  }
}
